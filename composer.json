{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.6.4",
    "VentureCraft/revisionable": "^1.27",
    "cmgmyr/messenger": "^2.13",
    "devster/ubench": "^1.2",
    "dingo/api": "1.0.x@dev",
    "doctrine/dbal": "2.5",
    "erusev/parsedown": "^1.6",
    "estgroupe/laravel-taggable": "^5.1",
    "guzzlehttp/guzzle": "^6.2",
    "hieu-le/active": "^3.4",
    "intervention/image": "^2.3",
    "jpush/jpush": "^3.2",
    "jrean/laravel-user-verification": "^2.2",
    "laracasts/flash": "^2.0",
    "laracasts/presenter": "^0.2.1",
    "laravel/framework": "^5.1",
    "laravel/socialite": "^2.0",
    "league/html-to-markdown": "^4.2",
    "lucadegasperi/oauth2-server-laravel": "^5.0",
    "mews/purifier": "^2.0",
    "naux/auto-correct": "^1.0",
    "naux/sendcloud": "^1.1",
    "nicolaslopezj/searchable": "^1.0",
    "orangehill/iseed": "^2.1",
    "overtrue/laravel-filesystem-qiniu": "^0.0.1",
    "overtrue/laravel-follow": "^1.1",
    "pda/pheanstalk": "^3.1",
    "predis/predis": "^1.1",
    "rap2hpoutre/laravel-log-viewer": "^0.7.0",
    "roumen/sitemap": "^2.6",
    "simplesoftwareio/simple-qrcode": "^1.4",
    "socialiteproviders/manager": "^2.1",
    "socialiteproviders/weixin": "^2.1",
    "spatie/laravel-backup": "^3.8",
    "spatie/laravel-pjax": "^1.3",
    "summerblue/administrator": "^1.0",
    "thujohn/rss": "*@dev",
    "zizaco/entrust": "^1.7"
  },
  "require-dev": {
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^0.9",
    "phpunit/phpunit": "^4.0",
    "phpspec/phpspec": "^2.1",
    "laralib/l5scaffold": "^1.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/",
      "Phphub\\": "app/Phphub/",
      "Modules\\": "Modules/"
    },
    "files": [
      "app/Helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "packages/rss"
    }
  ]
}
